{
    "buildCommand": "npm run build",
    "devCommand": "vite",
    "framework": "vite",
    "outputDirectory": "dist",
    "routes": [
      {
        "src": "/assets/(.*)",
        "headers": { "cache-control": "public, max-age=31536000, immutable" },
        "continue": true
      },
      { "handle": "filesystem" },
      { "src": "/.*", "dest": "/index.html" }
    ]
  }